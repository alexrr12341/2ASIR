#############################################################

create table pistas
(
CodInstal	varchar(2),
Superficie	integer,
Longitud	integer,
Tipo 		varchar(12),
foreign key (CodInstal) references instalaciones (CodInstal),
constraint ck_tipo check(Tipo IN ('Hierba','Arena Batida','Tierra'))
);

