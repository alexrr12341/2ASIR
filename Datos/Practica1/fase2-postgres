############################################################

create table carreras
(
CodCarrera		varchar(2),
Fecha 			date,
Hora 			time,
ImportePremio	integer,
LimiteApuesta	decimal(7,2),
FechaNacMin		date,
FechaNacMax		date,
primary key (CodCarrera),
constraint ck_hora check(TO_NUMBER(TO_CHAR(Hora,'HHMM'),'9999') BETWEEN 0900 AND 1400),
constraint ck_diaCarr check(to_char(Fecha,'D') LIKE '1'),
constraint ck_fecha check(to_number(to_char(Fecha,'MMDD'),'9999') BETWEEN 1020 AND 1231 OR to_number(to_char(Fecha,'MMDD'),'9999') BETWEEN 0101 AND 0302)
);

