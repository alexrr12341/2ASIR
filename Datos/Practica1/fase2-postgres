create table jockeys
(
DNI   		varchar(9),
Nombre 		varchar(20),
Altura		decimal(3,2),
Peso		decimal(3,1),
Telefono	varchar(12),
primary key (DNI),
constraint ck_dnijoc CHECK(DNI ~ '[0-9]{8}[A-Z]{1}')
);

